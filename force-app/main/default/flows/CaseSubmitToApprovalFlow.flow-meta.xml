        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Volta_Status</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>FALSE</defaultConnectorLabel>
        <rules>
            <name>TRUE_NonSubmitted</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.EnviadoParaAprovacao__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Submits_For_Approval</targetReference>
            </connector>
            <label>TRUE</label>
        </rules>
    </decisions>
