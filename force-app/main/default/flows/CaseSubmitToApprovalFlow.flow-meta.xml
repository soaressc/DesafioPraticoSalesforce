                    <stringValue>Agente de Suporte</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>NotSubmittedToApproval</targetReference>
            </connector>
            <label>TRUE</label>
        </rules>
    </decisions>
    <decisions>
        <name>NotSubmittedToApproval</name>
